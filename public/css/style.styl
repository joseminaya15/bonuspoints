		&.navbarcolor
			background-color #FFFFFF
			box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
			-webkit-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
			-moz-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
			.navbar-brand
				img
					max-width 200px
			.navbar-nav
				&>li
					&>a
						color #7c7c7c !important
	.navbar-brand
		img
			max-width 250px
	.navbar-default
		box-shadow none
		-webkit-box-shadow none
		-moz-box-shadow none
		.navbar-collapse
			margin-top 0
